# Load the mtcars dataset (if not already loaded)
data(mtcars)
# Perform linear regression (e.g., predicting mpg based on other variables)
model <- lm(mpg ~ wt + hp + qsec, data = mtcars)
# Display the summary of the regression model
summary(model)
# Load the mtcars dataset (if not already loaded)
data(mtcars)
# Calculate the total sales (number of cars) for each model
sales_data <- table(mtcars$mpg)
# Create a bar graph
barplot(sales_data,
main = "Sales of Cars (mtcars)",
xlab = "Miles per Gallon (mpg)",
ylab = "Number of Cars",
col = "skyblue",
border = "black",
names.arg = sort(unique(mtcars$mpg)),
cex.names = 0.7
)
# Add a legend
legend("topright", legend = "Car Models", fill = "skyblue", border = "black")
